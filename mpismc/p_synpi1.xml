			<!-- Binding rate of AraC::GFP to P4 promoter -->
		</prior>
		<prior>
			<index>2</index> <dist>uniform</dist> <lb>0.1</lb> <ub>30</ub>
			<!-- Dissociation rate of AraC::GFP to P4 promoter -->
			<!-- Units: 1/hour-->
			<!-- Dissociation constant Kd = k2/k1 of ... 1e-12 M (Schleif 1984 whole AraC) 1.4e-8 M (Schleif 2004 only DNA-binding domain + I1-I1 promoter) -->
			<!-- Half-life of 1.5 min to 7 hours -->
		</prior>
		<prior>
			<index>3</index> <dist>uniform</dist> <lb>1e-5</lb> <ub>1e-2</ub>
			<!-- Production rate of Cherry::LacI::LVA from uninduced (free) P4 promoter -->
			<!-- Units: molecules/(promoter*hour)-->
		</prior>
		<prior>
			<index>4</index> <dist>uniform</dist> <lb>1</lb> <ub>100</ub>
			<!-- Production rate of Cherry::LacI::LVA from induced P4 promoter -->
			<!-- Units: molecules/(promoter*hour)-->
		</prior>
		<prior>
			<index>5</index> <dist>uniform</dist> <lb>1</lb> <ub>5</ub>
			<!-- IPTG-independent degradation rate of Cherry::LacI::LVA protein -->
			<!-- Units: 1/hour -->
			<!-- Half-life of 1.5 min to 7 hours -->
		</prior>
		<prior>
			<index>6</index> <dist>uniform</dist> <lb>1</lb> <ub>100</ub>
			<!-- IPTG-dependent degradation rate of Cherry::LacI::LVA protein -->
			<!-- Units: 1/(IPTG*hour) -->
		</prior>
		<prior>
			<index>7</index> <dist>uniform</dist> <lb>1.6e-1</lb> <ub>3000</ub>
			<!-- Dimerization rate of Cherry::LacI::LVA protein -->
			<!-- Dissociation constant Kd = k7/k6 of 1e-9 M to 1e-13 M  ***RECALCULATE*** -->
		</prior>
		<prior>
			<index>8</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Dissociation rate of Cherry::LacI::LVA dimers -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>9</index> <dist>uniform</dist> <lb>1.6e-3</lb> <ub>1.6e0</ub>
			<!-- Binding rate of Cherry::LacI::LVA dimers to Ptac promoter -->
			<!-- Dissociation constant Kd = k9/k8 of 1e-7 M to 1e-10 M -->
		</prior>
		<prior>
			<index>10</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Dissociation rate of Cherry::LacI::LVA dimers from Ptac promoter -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>11</index> <dist>uniform</dist> <lb>1</lb> <ub>100</ub>
			<!-- Production rate of AraC::GFP from unoccupied Ptac promoter -->
			<!-- Units: molecules/(promoter*hour) -->
		</prior>
		<prior>
			<index>12</index> <dist>uniform</dist> <lb>1e-5</lb> <ub>1e-1</ub>
			<!-- Production rate of AraC::GFP from occupied Ptac promoter -->
			<!-- Units: molecules/(promoters*hour) -->
		</prior>
		<prior>
			<index>13</index> <dist>uniform</dist> <lb>1</lb> <ub>5</ub>
			<!-- Degradation rate of AraC::GFP protein -->
			<!-- Units: 1/hour -->
			<!-- Half-life of 1.5 min to 7 hours -->
		</prior>
	</priors>
