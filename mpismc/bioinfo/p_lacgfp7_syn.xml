<problem>
	<model>10</model>
	<!-- The model code 10 is LACGFP7 -->
	<samples>80000</samples>
	<outputs>1</outputs>
	<timepoints>5</timepoints>
	<parameters>18</parameters>
	<priors>
		<prior>
			<index>1</index> <dist>uniform</dist> <lb>1</lb> <ub>50</ub>
			<!-- Transcription rate of lacI mRNA -->
			<!-- Units: molecules/hour -->
			<!-- Pelechano et al. PLoS ONE 2010 found that transcription rate of >90% of YEAST genes is between 2 and 30 molecules/hour -->
			<!-- ATT!! Study was done in YEAST, slightly larger interval is used here -->
		</prior>
		<prior>
			<index>2</index> <dist>uniform</dist> <lb>0.1</lb> <ub>30</ub>
			<!-- Degradation rate of lacI mRNA -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 1.5 min to 7 hours -->
			<!-- The median half-life of an E. coli mRNA is 3.69 min (Bernstein et al. PNAS 2004) -->
		</prior>
		<prior>
			<index>3</index> <dist>uniform</dist> <lb>1</lb> <ub>50</ub>
			<!-- Translation rate of LACI protein -->
			<!-- Units: molecules/(mRNA*hour)-->
		</prior>
		<prior>
			<index>4</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Basal degradation rate of LACI protein -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>5</index> <dist>uniform</dist> <lb>1</lb> <ub>100</ub>
			<!-- Input-dependent degradation rate of LACI protein -->
			<!-- Units: 1/(IPTG*hour) -->
		</prior>
		<prior>
			<index>6</index> <dist>uniform</dist> <lb>1.6e-1</lb> <ub>3000</ub>
			<!-- Dimerization rate of LACI protein -->
			<!-- Dissociation constant Kd = k7/k6 of 1e-9 M to 1e-13 M  ***RECALCULATE*** -->
		</prior>
		<prior>
			<index>7</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Dissociation rate of LACI dimers -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>8</index> <dist>uniform</dist> <lb>1.6e-3</lb> <ub>1.6e0</ub>
			<!-- Binding rate of LACI dimers to free Lac operator sequences -->
			<!-- Dissociation constant Kd = k9/k8 of 1e-7 M to 1e-10 M -->
		</prior>
		<prior>
			<index>9</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Dissociation rate of LACI dimers from Lac operator sequence -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>10</index> <dist>uniform</dist> <lb>1.6e-1</lb> <ub>1.6e3</ub>
			<!-- Binding rate of two LACI dimer/operator to form a tetramer -->
			<!-- Dissociation constant Kd = k11/k10 of 1e-9 M to 1e-13 M -->
		</prior>
		<prior>
			<index>11</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Dissociation rate of LACI dimers from Lac operator sequence -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>12</index> <dist>uniform</dist> <lb>1</lb> <ub>150</ub>
			<!-- Transcription rate of gfp mRNA from unoccupied PLac-->
			<!-- Units: molecules/(promoter*hour) -->
		</prior>
		<prior>
			<index>13</index> <dist>uniform</dist> <lb>0.1</lb> <ub>15</ub>
			<!-- Transcription rate of gfp mRNA from O2Lac -->
			<!-- Units: molecules/(promoters*hour) -->
		</prior>
		<prior>
			<index>14</index> <dist>uniform</dist> <lb>1e-5</lb> <ub>1</ub>
			<!-- Transcription rate of gfp mRNA from O4Lac -->
			<!-- Units: molecules/(promoters*hour) -->
		</prior>
		<prior>
			<index>15</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Degradation rate of gfp mRNA -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>16</index> <dist>uniform</dist> <lb>1</lb> <ub>50</ub>
			<!-- Translation rate of dark GFP protein -->
			<!-- Units: molecules/(mRNA*hour)-->
		</prior>
		<prior>
			<index>17</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- Degradation rate of GFP protein -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
		<prior>
			<index>18</index> <dist>uniform</dist> <lb>0.1</lb> <ub>20</ub>
			<!-- GFP maturation time -->
			<!-- Units: 1/hour-->
			<!-- Half-life of 2 min to 7 hours -->
		</prior>
	</priors>
	<scaling>
		<mean>
			<index>1</index> <dist>uniform</dist> <lb>5</lb> <ub>200</ub>		
		</mean>
		<std>
			<index>1</index> <dist>uniform</dist> <lb>5</lb> <ub>200</ub>	
		</std>
	</scaling>
	<data mode = 'text' scale = 'linear'>
		<background>
			<fcsfile>/Users/lillaccg/Work/Code/C/2013/mpiStochEst/mpismc/fwt-bg.fcs</fcsfile>
			<measuredspecies>
				<measurement>
					<index>1</index> <description>GFP</description> <fcsparam>7</fcsparam>
				</measurement>		
			</measuredspecies>
		</background>
		<datafile>/Users/lillaccg/Work/Code/C/2013/mpiStochEst/syndata_lacgfp7.txt</datafile>
		<timept>
			<index>1</index> <time>0.0</time> <unit>hours</unit>
		</timept>
		<timept>
			<index>2</index> <time>2.0</time> <unit>hours</unit>
		</timept>
		<timept>
			<index>3</index> <time>3.0</time> <unit>hours</unit>
		</timept>
		<timept>
			<index>4</index> <time>4.0</time> <unit>hours</unit>
		</timept>
		<timept>
			<index>5</index> <time>5.0</time> <unit>hours</unit>
		</timept>
	</data>
</problem>